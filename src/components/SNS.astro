---
const { title, mb = "" } = Astro.props;
---

<div class="prose prose-xl container mx-auto max-w-5xl">
  <h2 class="flex w-full items-baseline justify-between space-x-2 jp-bold">
    {title}
  </h2>
  <div class="flex-row flex space-x-2">
    <a
      class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
      href="https://yasoprotivlenie.com"
      target="_blank"
    >
      <img
        class="my-0 h-12 w-12"
        src="data:image/png;base64,"
        alt="homepage"
      />
      <span class="text-xs">HP</span>
    </a>
    <a
      class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
      href="https://twitter.com/yakinikuprotiv"
      target="_blank"
    >
      <img
        class="my-0 h-12 w-12"
        src="data:image/png;base64,"
        alt="twitter"
      />
      <span class="text-xs">twitter</span>
    </a>
    <a
      class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
      href="https://www.instagram.com/ya.soprotivlenie/"
      target="_blank"
    >
      <img
        class="my-0 h-12 w-12"
        src="data:image/png;base64,"
        alt="instagram"
      />
      <span class="text-xs">insta</span>
    </a>
    <a
      class="flex flex-col items-center text-blue-500 no-underline print:visible print:h-8 print:w-8 print:text-black"
      href="https://t.me/yasoprotivlenie"
      target="_blank"
    >
      <img
        class="my-0 h-12 w-12"
        src="data:image/png;base64,"
        alt="telegram"
      />
      <span class="text-xs">tg</span>
    </a>
  </div>
</div>
